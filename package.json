{
	"name": "Raindrop.io",
	"productName": "Raindrop.io",
	"version": "0.0.0",
	"description": "Crossplatform bookmarking app",
	"main": "src/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/raindropio/desktop.git"
	},
	"scripts": {
		"dev": "electron-forge start",
		"build:mac": "electron-forge make --platform=darwin",
