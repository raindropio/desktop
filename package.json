	},
	"dependencies": {
		"@sentry/electron": "^2.0.1",
		"electron-context-menu": "3.1.x",
		"electron-is-dev": "2.x",
		"electron-squirrel-startup": "1.x",
		"electron-util": "0.17.x",
		"electron-window-state": "5.x",
		"lodash": "^4.17.20",
		"update-electron-app": "2.0.x"
	},
	"devDependencies": {
		"@electron-forge/cli": "6.0.0-beta.61",
		"@electron-forge/maker-appx": "6.0.0-beta.61",
		"@electron-forge/maker-dmg": "6.0.0-beta.61",
		"@electron-forge/maker-squirrel": "6.0.0-beta.61",
